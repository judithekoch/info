
public class Factorial {

	public int factorial(int n){
		int result = 1;
		for (int i = 1; i<=n; i++){
			result *= i;
		}
		return result;
	}
	
	public int fakultaet (int n) {
	     if (n>0) {
	          return n * fakultaet(n-1); // (direkte) Rekursion
	     } else {
	          return 1;
	     }
	}

	
}
